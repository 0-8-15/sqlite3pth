;; -*-Scheme-*-

(compile -s -O3 -d2 -J -emit-type-file "sqlite3pth.types" "sqlite3pth.scm" "sqlite/sqlite3.o" -L "-pthread")
(compile -c -O3 "sqlite3pth.scm" "sqlite/sqlite3.o" -unit sqlite3pth -L "-pthread")
(compile -s -O2 -d0 "sqlite3pth.import.scm")

(install-extension
 'sqlite3pth
 '("sqlite3pth.so" "sqlite3pth.import.so" "sqlite3pth.types")
 '((version "0.2")))
